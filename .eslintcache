ctions.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authConstants.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestMap.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedMap.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/async/asyncReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/util/util.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/api/mockApi.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/layout/LoadingComponent.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDashboard/EventListItemPlaceholder.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDashboard/EventFilters.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/firestore/firestoreService.js",["285"],"/* eslint-disable no-useless-catch */\n/* eslint-disable no-prototype-builtins */\nimport cuid from 'cuid';\nimport firebase from '../config/firebase';\n\nconst db = firebase.firestore();\n\nexport const dataFromSnapshot = (snapshot) => {\n  if (!snapshot.exists) return undefined;\n  const data = snapshot.data();\n\n  for (const prop in data) {\n    if (data.hasOwnProperty(prop)) {\n      if (data[prop] instanceof firebase.firestore.Timestamp) {\n        data[prop] = data[prop].toDate();\n      }\n    }\n  }\n\n  return {\n    ...data,\n    id: snapshot.id,\n  };\n};\n\nexport const ListenToEventsFromFirestore = () => {\n  return db.collection('events');\n};\n\nexport const ListenToEventFromFirestore = (eventId) => {\n  return db.collection('events').doc(eventId);\n};\n\nexport const addEventToFirestore = (event) => {\n  return db.collection('events').add({\n    ...event,\n    hostedBy: 'Diana',\n    hostPhotoURL: 'https://randomuser.me/api/portraits/women/20.jpg',\n    attendees: firebase.firestore.FieldValue.arrayUnion({\n      id: cuid(),\n      displayName: 'Diana',\n      photoURL: 'https://randomuser.me/api/portraits/women/20.jpg',\n    }),\n  });\n};\n\nexport const updateEventInFirestore = (event) => {\n  return db.collection('events').doc(event.id).update(event);\n};\n\nexport const deleteEventInFirestore = (eventId) => {\n  return db.collection('events').doc(eventId).delete();\n};\n\nexport const cancelEventToggle = (event) => {\n  return db.collection('events').doc(event.id).update({\n    isCancelled: !event.isCancelled,\n  });\n};\n\nexport const setUserProfileData = (user) => {\n  return db\n    .collection('users')\n    .doc(user.uid)\n    .set({\n      displayName: user.displayName,\n      email: user.email,\n      photoURL: user.photoURL || null,\n      createAt: firebase.firestore.FieldValue.serverTimestamp(),\n    });\n};\n\nexport const getUserProfile = (userId) => {\n  return db.collection('users').doc(userId);\n};\n\nexport const updateUserProfile = async (profile) => {\n  const user = firebase.auth().currentUser;\n\n  try {\n    if (user.displayName !== profile.displayName) {\n      await user.updateProfile({\n        displayName: profile.displayName,\n      });\n    }\n    return await db.collection('users').doc(user.uid).update(profile);\n  } catch (error) {\n    throw error;\n  }\n};\n\nexport const updateUserProfilePhoto = async (downloadURL, filename) => {\n  const user = firebase.auth().currentUser;\n  const userDocRef = db.collection('users').doc(user.uid);\n  try {\n    const userDoc = await userDocRef.get();\n    console.log(userDoc);\n    if (!userDoc.data().photoURL) {\n      await db.collection('users').doc(user.uid).update({\n        photoURL: downloadURL,\n      });\n      await user.updateProfile({\n        photoURL: downloadURL,\n      });\n    }\n    return await db.collection('users').doc(user.uid).collection('photos').add({\n      name: filename,\n      url: downloadURL,\n    });\n  } catch (error) {\n    throw error;\n  }\n};\n\nexport const getUserPhotos = (userUid) => {\n  return db.collection('users').doc(userUid).collection('photos');\n};\n","/home/pedro/node/react/react-semantic-ui/src/app/config/firebase.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/hooks/useFirestoreCollection.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/hooks/useFirestoreDoc.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/error/ErrorComponent.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestPlaceInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyPlaceInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/firestore/firebaseService.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/RegisterForm.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/SocialLogin.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/AccountPage.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/ProfilePage.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/ProfileHeader.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/ProfileContent.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/profileReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/profileConstants.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/profileActions.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/AboutTab.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/ProfileForm.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/profiles/profilePage/PhotoTab.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/photos/PhotoUploadWidget.jsx",["286"],"import React, { useState } from 'react';\nimport { Button, Grid, Header } from 'semantic-ui-react';\nimport PhotoWidgetCropper from './PhotoWidgetCropper';\nimport PhotoWidgetDropzone from './PhotoWidgetDropzone';\nimport cuid from 'cuid';\nimport { getFileExtension } from '../util/util';\nimport { uploadToFirebaseStorage } from '../../firestore/firebaseService';\nimport { toast } from 'react-toastify';\nimport { updateUserProfilePhoto } from '../../firestore/firestoreService';\n\nconst initialState = [];\n\nconst PhotoUploadWidget = ({ setEditMode }) => {\n  const [files, setFiles] = useState(initialState);\n  const [image, setImage] = useState(null);\n  const [loading, setLoading] = useState(false);\n\n  const handldeUploadImage = () => {\n    setLoading(true);\n    const filename = cuid() + '.' + getFileExtension(files[0].name);\n    const uploadTask = uploadToFirebaseStorage(image, filename);\n    uploadTask.on(\n      'state_changed',\n      (snapshot) => {\n        const progress = (snapshot.bytesTransferred / snapshot.totalBytes) * 100;\n        console.log('Upload is ' + progress + '% done');\n      },\n      (error) => {\n        toast.error(error.message);\n      },\n      () => {\n        uploadTask.snapshot.ref.getDownloadURL().then((downloadURL) => {\n          updateUserProfilePhoto(downloadURL, filename)\n            .then(() => {\n              setLoading(false);\n              handleCancelCrop();\n              setEditMode(false);\n            })\n            .catch((error) => {\n              toast.error(error.message);\n              setLoading(false);\n            });\n        });\n      }\n    );\n  };\n\n  const handleCancelCrop = () => {\n    setFiles([]);\n    setImage(null);\n  };\n\n  return (\n    <Grid>\n      <Grid.Column width={4}>\n        <Header color=\"teal\" sub content=\"Step 1 - Add Photo\" />\n        <PhotoWidgetDropzone setFiles={setFiles} />\n      </Grid.Column>\n      <Grid.Column width={1} />\n      <Grid.Column width={4}>\n        <Header color=\"teal\" sub content=\"Step 2 - Resize\" />\n        {files.length > 0 && (\n          <PhotoWidgetCropper setImage={setImage} imagePreview={files[0].preview} />\n        )}\n      </Grid.Column>\n      <Grid.Column width={1} />\n      <Grid.Column width={4}>\n        <Header color=\"teal\" sub content=\"Step 3 - Preview & upload\" />\n        {files.length > 0 && (\n          <>\n            <div\n              className=\"img-preview\"\n              style={{ minHeight: 200, minWidth: 200, overflow: 'hidden' }}\n            />\n            <Button.Group>\n              <Button\n                loading={loading}\n                onClick={handldeUploadImage}\n                style={{ width: 100 }}\n                positive\n                icon=\"check\"\n              />\n              <Button\n                disabled={loading}\n                onClick={handleCancelCrop}\n                style={{ width: 100 }}\n                icon=\"close\"\n              />\n            </Button.Group>\n          </>\n        )}\n      </Grid.Column>\n    </Grid>\n  );\n};\n\nexport default PhotoUploadWidget;\n","/home/pedro/node/react/react-semantic-ui/src/app/common/photos/PhotoWidgetDropzone.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/photos/PhotoWidgetCropper.jsx",["287"],"import React, { useRef } from 'react';\nimport Cropper from 'react-cropper';\nimport 'cropperjs/dist/cropper.css';\n\nconst PhotoWidgetCropper = ({ setImage, imagePreview }) => {\n  const cropperRef = useRef(null);\n\n  const cropImage = () => {\n    // if (typeof cropperRef.current.getCroppedCanvas() === 'undefined') {\n    //   console.log('salir');\n    //   return;\n    // }\n\n    const imageElement = cropperRef?.current;\n    const cropper = imageElement?.cropper;\n    console.log(cropper.getCroppedCanvas().toDataURL());\n    // cropper.current.getCroppedCanvas().toBlob((blob) => {\n    cropper.getCroppedCanvas().toBlob((blob) => {\n      // console.log(blob);\n      setImage(blob);\n    }, 'image/jpeg');\n  };\n\n  return (\n    <Cropper\n      ref={cropperRef}\n      src={imagePreview}\n      style={{ height: 200, width: '100%' }}\n      // Cropper.js options\n      // initialAspectRatio={1}\n      aspectRatio={1}\n      preview=\".img-preview\"\n      guides={true}\n      viewMode={1}\n      dragMode=\"move\"\n      scalable={true}\n      cropBoxMovable={true}\n      cropBoxResizable={true}\n      crop={cropImage}\n    />\n  );\n};\n\nexport default PhotoWidgetCropper;\n",{"ruleId":"288","severity":1,"message":"289","line":17,"column":8,"nodeType":"290","messageId":"291","endLine":17,"endColumn":24},{"ruleId":"288","severity":1,"message":"292","line":24,"column":11,"nodeType":"290","messageId":"291","endLine":24,"endColumn":22},{"ruleId":"293","severity":1,"message":"294","line":97,"column":5,"nodeType":"295","messageId":"296","endLine":97,"endColumn":16},{"ruleId":"293","severity":1,"message":"294","line":26,"column":9,"nodeType":"295","messageId":"296","endLine":26,"endColumn":20},{"ruleId":"293","severity":1,"message":"294","line":16,"column":5,"nodeType":"295","messageId":"296","endLine":16,"endColumn":16},"no-unused-vars","'LoadingComponent' is defined but never used.","Identifier","unusedVar","'initialized' is assigned a value but never used.","no-console","Unexpected console statement.","MemberExpression","unexpected"]