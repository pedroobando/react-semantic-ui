act-redux';\nimport { createEvent, listenToEvents, updateEvent } from '../eventActions';\nimport { categoryData } from '../../../app/api/categoryOptions';\nimport { useFirestoreDoc } from '../../../app/hooks/useFirestoreDoc';\nimport { ListenToEventFromFirestore } from '../../../app/firestore/firestoreService';\n\nimport cuid from 'cuid';\nimport * as Yup from 'yup';\n\nimport MyTextInput from '../../../app/common/form/MyTextInput';\nimport MyTextArea from '../../../app/common/form/MyTextArea';\nimport MySelectInput from '../../../app/common/form/MySelectInput';\nimport MyDateInput from '../../../app/common/form/MyDateInput';\n\nconst EventForm = ({ match, history }) => {\n  const dispatch = useDispatch();\n  const selectedEvent = useSelector((state) =>\n    state.event.events.find((evt) => evt.id == match.params.id)\n  );\n\n  console.log(selectedEvent);\n\n  const initialValues = selectedEvent ?? {\n    title: '',\n    category: '',\n    description: '',\n    city: '',\n    venue: '',\n    date: '',\n  };\n\n  const validationSchema = Yup.object({\n    title: Yup.string().required('You must provide a title'),\n    category: Yup.string().required('You must provide a category'),\n    description: Yup.string().required('You must provide a description'),\n    city: Yup.string().required(),\n    venue: Yup.string().required(),\n    date: Yup.string().required(),\n  });\n\n  useFirestoreDoc({\n    query: () => ListenToEventFromFirestore(match.params.id),\n    data: (event) => dispatch(listenToEvents([event])),\n    deps: [match.params.id, dispatch],\n  });\n\n  return (\n    <Segment clearing>\n      <Formik\n        initialValues={initialValues}\n        validationSchema={validationSchema}\n        onSubmit={(values) => {\n          selectedEvent\n            ? dispatch(updateEvent({ ...selectedEvent, ...values }))\n            : dispatch(\n                createEvent({\n                  ...values,\n                  id: cuid(),\n                  hostedBy: 'Bob',\n                  attendees: [\n                    {\n                      id: cuid(),\n                      photoURL: '/assets/user.png',\n                      name: 'moli',\n                    },\n                  ],\n                  hostPhotoURL: '/assets/user.png',\n                })\n              );\n          history.push('/events');\n        }}>\n        {({ isSubmitting, dirty, isValid }) => (\n          <Form className=\"ui form\">\n            <Header sub color=\"teal\" content=\"Event Details\" />\n            <MyTextInput name=\"title\" placeholder=\"Event title\" />\n            <MySelectInput name=\"category\" placeholder=\"Category\" options={categoryData} />\n            <MyTextArea name=\"description\" placeholder=\"Description\" rows={3} />\n            <Header sub color=\"teal\" content=\"Event Location Details\" />\n            <MyTextInput name=\"city\" placeholder=\"City\" />\n            <MyTextInput name=\"venue\" placeholder=\"Venue\" />\n            <MyDateInput\n              name=\"date\"\n              placeholderText=\"Event date\"\n              timeFormat=\"HH:mm\"\n              showTimeSelect\n              timeCaption=\"time\"\n              dateFormat=\"MMMM d, yyyy h:mm a\"\n            />\n            <Button\n              loading={isSubmitting}\n              disabled={!isValid || !dirty || isSubmitting}\n              type=\"submit\"\n              floated=\"right\"\n              positive\n              content=\"Submit\"\n            />\n            <Button\n              disabled={isSubmitting}\n              as={Link}\n              to=\"/events\"\n              type=\"button\"\n              floated=\"right\"\n              content=\"Cancel\"\n            />\n          </Form>\n        )}\n      </Formik>\n    </Segment>\n  );\n};\n\nexport default EventForm;\n","/home/pedro/node/react/react-semantic-ui/src/features/events/eventDashboard/EventListItem.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/home/HomePage.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/api/sampleData.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/nav/SignedInMenu.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/nav/SignedOutMenu.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedPage.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedInfo.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedHeader.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedSidebar.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/layout/App.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedChat.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/store/configureStore.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/testRedux.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/Sandbox.jsx",["210"],"import React, { useEffect, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Button, Input, Segment } from 'semantic-ui-react';\nimport { openModal } from '../../app/common/modals/modalReducer';\nimport { TestMap } from './TestMap';\nimport { decrement, increment } from './testRedux';\n\nimport { OpenStreetMapProvider } from 'leaflet-geosearch';\n\nconst initialStateMapLocation = {\n  address: '',\n  latLng: {\n    lat: 0,\n    lng: 0,\n  },\n  zoom: 15,\n  scrollZoom: true,\n};\n\nconst provider = new OpenStreetMapProvider();\n\nconst Sandbox = () => {\n  const dispatch = useDispatch();\n  const [target, setTarget] = useState(null);\n  const data = useSelector((state) => state.test.data);\n  const { loading } = useSelector((state) => state.async);\n\n  const [mapLocation, setMapLocation] = useState(initialStateMapLocation);\n\n  // useEffect(async () => {\n  //   const streeSearch = mapLocation.address.trim();\n  //   if (streeSearch.length >= 3) {\n  //     const srcLocation = await provider.search({ query: mapLocation.address.trim() });\n  //     if (srcLocation.length >= 1)\n  //       setMapLocation((valueMap) => ({\n  //         ...valueMap,\n  //         latLng: { lat: srcLocation[0].y, lng: srcLocation[0].x },\n  //       }));\n  //   }\n  // }, [mapLocation.address]);\n\n  const handleChangeMapLocation = ({ target }) => {\n    setMapLocation((valueMap) => ({ ...valueMap, [target.name]: target.value }));\n  };\n\n  const handleSearchAddress = async () => {\n    const streeSearch = mapLocation.address.trim();\n    if (streeSearch.length >= 3) {\n      const srcLocation = await provider.search({ query: mapLocation.address.trim() });\n      if (srcLocation.length >= 1)\n        setMapLocation((valueMap) => ({\n          ...valueMap,\n          latLng: { lat: srcLocation[0].y, lng: srcLocation[0].x },\n        }));\n    }\n  };\n\n  return (\n    <>\n      <h1>Testing 123</h1>\n      <h2>The data is: {data}</h2>\n      <Button\n        name=\"increment\"\n        loading={loading && target === 'increment'}\n        onClick={(e) => {\n          dispatch(increment(10));\n          setTarget(e.target.name);\n        }}\n        content=\"Increment\"\n        color=\"green\"\n      />\n      <Button\n        name=\"decrement\"\n        loading={loading && target === 'decrement'}\n        onClick={(e) => {\n          dispatch(decrement(20));\n          setTarget(e.target.name);\n        }}\n        content=\"Decrement\"\n        color=\"red\"\n      />\n      <Button\n        onClick={() => dispatch(openModal({ modalType: 'TestModal', modalProps: { data } }))}\n        content=\"Open Modal\"\n        color=\"teal\"\n      />\n      <hr />\n      <Input\n        placeholder=\"indique el lugar\"\n        name=\"address\"\n        value={mapLocation.address}\n        onChange={handleChangeMapLocation}\n        style={{ width: '300px' }}\n      />\n\n      <Button color=\"violet\" onClick={handleSearchAddress} style={{ margin: '0px 5px' }}>\n        Buscar\n      </Button>\n\n      <label name=\"coord\">{JSON.stringify(mapLocation.latLng)}</label>\n\n      <Segment style={{ width: '100%', height: '500px', marginTop: '20px' }}>\n        <TestMap defaultProps={mapLocation} setLatLng={setMapLocation} />\n      </Segment>\n    </>\n  );\n};\n\nexport default Sandbox;\n","/home/pedro/node/react/react-semantic-ui/src/app/store/rootReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventConstants.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventActions.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/layout/ScrollToTop.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyTextInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyTextArea.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/api/categoryOptions.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MySelectInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyDateInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/modals/modalReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/modals/ModalManager.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestModal.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/modals/ModalWrapper.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/LoginForm.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authActions.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authConstants.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestMap.jsx",["211","212","213","214