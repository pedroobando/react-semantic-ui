ode/react/react-semantic-ui/src/app/common/form/MyTextInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyTextArea.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/api/categoryOptions.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MySelectInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyDateInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/modals/modalReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/modals/ModalManager.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestModal.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/modals/ModalWrapper.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/LoginForm.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authActions.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authConstants.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/auth/authReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestMap.jsx",["218","219","220","221"],"import React, { useState, useEffect } from 'react';\nimport { MapContainer, TileLayer, Marker, Popup, useMapEvents } from 'react-leaflet';\n\nconst initialMarkState = {\n  fixed: false,\n  latlng: { lat: 0, lng: 0 },\n};\n\nexport const TestMap = ({ defaultProps, setLatLng }) => {\n  // const [positionMap, setPositionMap] = useState(initialMarkState);\n  const { address, latLng, zoom, scrollZoom } = defaultProps;\n\n  if (latLng.lat === -999 && latLng.lng === -999) return <div></div>;\n\n  const LocationMarker = () => {\n    const map = useMapEvents({\n      click: (e) => {\n        setLatLng((locMap) => ({ ...locMap, latLng: e.latlng }));\n      },\n    });\n\n    useEffect(() => {\n      map.flyTo(latLng, map.getZoom());\n    }, [latLng]);\n\n    return latLng === null ? null : (\n      <Marker position={latLng}>\n        <Popup>{address}</Popup>\n      </Marker>\n    );\n  };\n\n  return (\n    <MapContainer\n      center={latLng}\n      zoom={5}\n      scrollWheelZoom={false}\n      style={{ width: '100%', height: '100%', zIndex: 2 }}>\n      <TileLayer\n        attribution='&copy; <a href=\"http://osm.org/copyright\">OpenStreetMap</a> contributors'\n        url=\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\"\n      />\n      <LocationMarker />\n    </MapContainer>\n  );\n};\n","/home/pedro/node/react/react-semantic-ui/src/features/events/eventDetailed/EventDetailedMap.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/async/asyncReducer.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/util/util.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/api/mockApi.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/layout/LoadingComponent.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDashboard/EventListItemPlaceholder.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/events/eventDashboard/EventFilters.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/firestore/firestoreService.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/config/firebase.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/hooks/useFirestoreCollection.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/hooks/useFirestoreDoc.js",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/error/ErrorComponent.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/features/sandox/TestPlaceInput.jsx",[],"/home/pedro/node/react/react-semantic-ui/src/app/common/form/MyPlaceInput.jsx",["222","223","224","225"],"import React, { useEffect, useRef, useState } from 'react';\nimport { useField, useFormikContext } from 'formik';\nimport { FormField, List, Segment, Label } from 'semantic-ui-react';\nimport { OpenStreetMapProvider } from 'leaflet-geosearch';\n\nconst initialState = {\n  address: '',\n  latlng: {\n    lat: 0,\n    lng: 0,\n  },\n};\n\nconst initialStateList = [];\nconst provider = new OpenStreetMapProvider();\n\nconst MyPlaceInput = ({ onSelect, ...props }) => {\n  const [field, meta, helpers] = useField(props);\n  const [addressList, setAddressList] = useState(initialStateList);\n\n  const handleChangeAddress = ({ target }) => {\n    // setMapLocation((valueMap) => ({ ...valueMap, address: target.value }));\n    helpers.setValue({ address: target.value });\n  };\n\n  const handleSearchAddress = async () => {\n    const streeSearch = field.value['address'];\n    if (streeSearch.length >= 3) {\n      const srcLocation = await provider.search({ query: streeSearch });\n      if (srcLocation.length >= 1) {\n        setAddressList(\n          srcLocation.map((point) => ({\n            address: point.label,\n            latlng: { lat: point.y, lng: point.x },\n          }))\n        );\n      }\n    }\n  };\n\n  const handleSelectPlace = (place) => {\n    setAddressList(initialStateList);\n    helpers.setValue({\n      address: place.address,\n      latlng: {\n        lat: place.latlng.lat,\n        lng: place.latlng.lng,\n      },\n    });\n    onSelect({ fieldName: field.name, ...place });\n  };\n\n  return (\n    <>\n      <FormField error={meta.touched && !!meta.error}>\n        <div className=\"ui icon input\" style={{ width: '100%' }}>\n          <input {...field} value={field.value['address']} onChange={handleChangeAddress} />\n          <i\n            className=\"cancel link icon\"\n            onClick={() => {\n              setAddressList(initialStateList);\n            }}></i>\n          <i\n            className=\"circular search link icon\"\n            style={{ marginRight: '30px' }}\n            onClick={handleSearchAddress}></i>\n        </div>\n        {meta.touched && meta.error ? (\n          <Label basic color=\"red\">\n            {meta.error}\n          </Label>\n        ) : null}\n\n        {addressList.length > 0 && (\n          <Segment style={{ margin: '0px auto', padding: '5px 10px' }}>\n            <List selection>\n              {addressList.map((place, indk) => (\n                <List.Item\n                  key={indk}\n                  icon=\"point\"\n                  content={place.address}\n                  onClick={() => handleSelectPlace(place)}\n                  style={{ cursor: 'pointer' }}\n                />\n              ))}\n            </List>\n          </Segment>\n        )}\n      </FormField>\n    </>\n  );\n};\n\nexport default MyPlaceInput;\n",{"ruleId":"226","severity":1,"message":"227","line":55,"column":5,"nodeType":"228","messageId":"229","endLine":55,"endColumn":16},{"ruleId":"230","severity":1,"message":"231","line":1,"column":17,"nodeType":"232","messageId":"233","endLine":1,"endColumn":25},{"ruleId":"230","severity":1,"message":"234","line":4,"column":7,"nodeType":"232","messageId":"233","endLine":4,"endColumn":23},{"ruleId":"230","severity":1,"message":"235","line":11,"column":28,"nodeType":"232","messageId":"233","endLine":11,"endColumn":32},{"ruleId":"230","severity":1,"message":"236","line":11,"column":34,"nodeType":"232","messageId":"233","endLine":11,"endColumn":44},{"ruleId":"230","severity":1,"message":"237","line":1,"column":17,"nodeType":"232","messageId":"233","endLine":1,"endColumn":26},{"ruleId":"230","severity":1,"message":"238","line":1,"column":28,"nodeType":"232","messageId":"233","endLine":1,"endColumn":34},{"ruleId":"230","severity":1,"message":"239","line":2,"column":20,"nodeType":"232","messageId":"233","endLine":2,"endColumn":36},{"ruleId":"230","severity":1,"message":"240","line":6,"column":7,"nodeType":"232","messageId":"233","endLine":6,"endColumn":19},"no-console","Unexpected console statement.","MemberExpression","unexpected","no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'initialMarkState' is assigned a value but never used.","'zoom' is assigned a value but never used.","'scrollZoom' is assigned a value but never used.","'useEffect' is